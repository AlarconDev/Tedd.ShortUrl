{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "Server=tcp:***,1433;Initial Catalog=***;Persist Security Info=False;User ID=***;Password=***;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "Create": {
    "KeyLength": 6,
    "_hint": "Excluding chars that can be confused with others",
    "KeyChars": "23456789abcdefghjkmnpqrstuvwxyzABCDEFGHJKLMNPQRSTUVWXYZ"
  },
  "Security": {
    "AuthenticationTokens": [ "ValidToken123" ],
    "RequireTokenForCreate": true,
    "RequireTokenForGet": true
  },
  "Cache": {
    "ItemLimit": 10000,
    "NegativeCacheTimeoutSeconds": 60
  }
}
